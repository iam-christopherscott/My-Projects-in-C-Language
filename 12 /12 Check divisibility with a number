// Check divisibility with a number.
{
    int a;
    printf("Enter a number\n");
    scanf("%d", &a);
    int b;
    printf("Enter another number to check its divisibility with the previous number\n");
    scanf("%d", &b);
    printf("Answer: 0 = Divisible; 1 = Not Divisible\n %d", a%b);
}
